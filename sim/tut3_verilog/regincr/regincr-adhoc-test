E_0xdb4270 .event anyedge, v0xdc8730_0;
E_0xdb45e0 .event posedge, v0xdb3200_0;
    .scope S_0xdb2fe0;
T_0 ;
    %wait E_0xdb45e0;
    %load/vec4 v0xdc8810_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0xdc8730_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0xdc8590_0;
    %assign/vec4 v0xdc8730_0, 0;
T_0.1 ;
    %jmp T_0;
